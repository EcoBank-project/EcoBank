<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ecobank.app.intro.mapper.CarbonMapper">
	<!-- 챌린지 전체 조회 -->
	<select id="selectCarbonAll" resultType="com.ecobank.app.intro.service.CarbonVO">
		SELECT carbon_no,
			   region_code,
			   nation_code,
			   NVL(carbon_emissions,0) as carbon_emissions 
		FROM carbon 
		ORDER BY carbon_emissions DESC
	</select>
</mapper>
	